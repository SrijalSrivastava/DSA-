import java.util.*;
import java.lang.Math;
class Main{
    public static void main(String [] args){
     Scanner sc =  new Scanner(System.in);
     int n = sc.nextInt();
     double ans =0; 
     int i=0;
    
    while(n!=0){
        int digit = n%10;
        if(digit == 1){
        ans = ans + Math.pow(2,i);
        }
        n = n/10;
        i++;
    }
      System.out.println(ans);
    }
}
