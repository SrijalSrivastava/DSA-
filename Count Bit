Count number of Bit in an Integer number ;
import java.util.*;
class Main{
    public static void main(String [] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();

        int count = 0; 
         while(n!=0){
            int rem = n%2 ;
            if(rem == 1){
                count ++;
            }
           n = n/2;
         }
         System.out.println(count);
    }
}
